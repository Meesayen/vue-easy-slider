{
  "name": "vue-easy-slider",
  "version": "5.4.0-next.0",
  "description": "vue-easy-slider is a slider component of Vue.js.",
  "keywords": [
    "slide",
    "swipe",
    "carrousel",
    "vue-swipe",
    "vue-swiper",
    "vue-slider",
    "vue-component"
  ],
  "homepage": "https://github.com/meesayen/vue-easy-slider",
  "author": "Suhaha < jklopsdfw@gmail.com >",
  "repository": {
    "type": "git",
    "url": "git@github.com:meesayen/vue-easy-slider.git"
  },
  "license": "MIT",
  "main": "dist/vue-easy-slider.umd.js",
  "module": "dist/vue-easy-slider.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-easy-slider.es.js",
      "require": "./dist/vue-easy-slider.umd.js"
    }
  },
  "unpkg": "dist/vue-easy-slider.min.js",
  "jsdelivr": "dist/vue-easy-slider.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "build:browser": "cross-env NODE_ENV=production rollup --config build/rollup.config.browser.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.esm.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.browser.js --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "^5.2.2",
    "flush-promises": "^1.0.2",
    "jest": "^24.8.0",
    "sass": "^1.35.1",
    "vite": "^2.3.8",
    "vue": "^3.1.0",
    "vue-jest": "^3.0.4"
  }
}
